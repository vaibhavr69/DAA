#include<stdio.h>

int binary_search(int array[],int n,int key)
{
    int i;
    int s=0;
    int e=n;
    while(s<=e)
    {
        int mid=(s+e)/2;
        
        if(mid==key)
        {
            return mid;
        }
        else if(mid>key)
        {
            return mid-1;
        }
        else if(mid<key)
        {
            return mid+1;
        }
    }
    return -1;
}

int main()
{
    int n,i,key;
    int array[n];
    printf("Enter the no. of elements you want in your array : ");
    scanf("%d",&n);
    printf("Enter the elements in your array : ");
    for(i=0;i<n;i++)
    {
        scanf("%d",&array[i]);
    }
    
    printf("Enter the key : ");
    scanf("%d",&key);
    
    printf("Present %d",binary_search(array,n,key));
}

