#include <iostream>
using namespace std;
int main()
{
 int t;
 cout<<"Enter number of test cases\n";
 cin>>t;
 while(t>0)
 {
     int n,key,count=0,flag=0;
     int A[30];
     cout<<"Enter size of array\n";
     cin>>n;
     cout<<"Enter elements in the array\n";
     for(int i=0;i<n;i++)
     cin>>A[i];

     cout<<"Enter key to be searched\n";
     cin>>key;
     for(int i=0;i<n;i++)
     {
         count++;
         if(A[i]==key)
         {
             cout<<"found\n";
             flag=1;
             break;
         }
     }
     if(flag==0)
     cout<<"Element not found\n";
     t--;
     cout<<"Number of iterations: "<<count<<endl;

 }
}
